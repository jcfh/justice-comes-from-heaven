#include<stdio.h>
main()
{
	int a,b,m,n,x,y;
        printf("请输入整数");
	do{
	scanf("%d %d",&a,&b);  
		if(a>b)
		{
			m=a-b;
			if(m>b)
			{
				for(n=b;n>1;n--)
				{
					x=m%n;y=b%n;
					if(x==0&&y==0)
					{
					printf("%d\n",n);
					n=1;
					}
					else
					if(n==2)
					{
					printf("无公约数\n");
					} 
				}
			}
			if(m<b)
			{
				for(n=b;n>1;n--)
				{
					x=m%n;y=b%n;
					if(x==0&&y==0)
					{
					printf("%d\n",n);
					n=1;
					}
					else
					if(n==2)
					{
					printf("无公约数\n");
					}
				}
			}
			if(m==b)
			{
				for(n=b;n>1;n--)
				{
					x=m%n;
					if(x==0)
					{
					printf("%d\n",n);
					n=1;
					}
					else
					if(n==2)
					{
					printf("无公约数\n");
					} 
				}
			}
		}
		if(a<b)
		{
			m=b-a;
			if(m>a)
			{
				for(n=a;n>1;n--)
				{
					x=m%n;y=a%n;
					if(x==0&&y==0)
					{
					printf("%d\n",n);
					n=1;
					}
					else
					if(n==2)
					{
					printf("无公约数\n");
					} 
				}
			}
			if(m<a)
			{
				for(n=a;n>1;n--)
				{
					x=m%n;y=a%n;
					if(x==0&&y==0)
					{
					printf("%d\n",n);
					n=1;
					}
					else
					if(n==2)
					{
					printf("无公约数\n");
					}
				}
			}
			if(m==a)
			{
				for(n=a;n>1;n--)
				{
					x=m%n;
					if(x==0)
					{
					printf("%d\n",n);
					n=1;
					}
					else
					if(n==2)
					{
					printf("无公约数\n");
					} 
				}
			}			
		}
		if(a==b&&a!=0)
		{
			printf("%d\n",a);
		}
	}while(a!=0&&b!=0);
}
